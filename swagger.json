  "paths": {
    "/api/v1/auth/token": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "Creates an API-Key for accessing the FLOWWER-API",
        "description": "Send this API-Key with your API-calls by using the HTTP-Header 'X-FLOWWER-ApiKey' for access to non-anonymous API-Actions.",
        "operationId": "POST-auth-token",
        "produces": [
          "text/plain",
          "application/json",
          "text/json"
        ],
        "parameters": [
          {
            "in": "header",
            "name": "username",
            "description": "The API-Key will be created for this user.",
            "required": true,
            "type": "string"
          },
          {
            "in": "header",
            "name": "password",
            "description": "The user's password.",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "API-Key",
            "schema": {
              "type": "string"
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        },
        "deprecated": true,
        "x-ms-visibility": "important"
      }
    },
    "/api/v1/companies/activeflows/reduced": {
      "get": {
        "tags": [
          "Companies"
        ],
        "summary": "Get all active companies with valid flowws for user (reduced/simple model)",
        "description": "API-Cooldown: 3 Seconds. Requires appropriate permissions.",
        "operationId": "GetAllCompaniesWithValidFlowsReduced",
        "produces": [
          "text/plain",
          "application/json",
          "text/json"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/CompanyAndFlowInfoReduced"
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          }
        },
        "security": [
          {
            "ValidateFlowwerAuthenticationScheme": [ ]
          }
        ],
        "x-ms-visibility": "important"
      }
    },
    "/api/v1/companies/{CompanyId}/activeflows/reduced": {
      "get": {
        "tags": [
          "Companies"
        ],
        "summary": "Get valid flowws for a company (reduced/simple model)",
        "description": "API-Cooldown: 1 Second. Requires appropriate permissions.",
        "operationId": "GetValidFlowsOfCompany",
        "produces": [
          "text/plain",
          "application/json",
          "text/json"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "CompanyId",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/FlowInfoReduced"
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          }
        },
        "security": [
          {
            "ValidateFlowwerAuthenticationScheme": [ ]
          }
        ],
        "x-ms-visibility": "important"
      }
    },
    "/api/v1/documents/all": {
      "get": {
        "tags": [
          "Documents"
        ],
        "summary": "Get a list of all documents",
        "description": "Requires AdministerAllDocuments permission.",
        "operationId": "GetAllDocuments",
        "produces": [
          "text/plain",
          "application/json",
          "text/json"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "includeProcessed",
            "description": "Also include processed documents in the result.",
            "type": "boolean"
          },
          {
            "in": "query",
            "name": "includeDeleted",
            "description": "Also include deleted documents in the result.",
            "type": "boolean"
          }
        ],
        "responses": {
          "401": {
            "description": "Unauthorized"
          },
          "200": {
            "description": "Successful operation. NOTE: Response-Model is planned to be simplified!",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/DocumentStatusInfo"
              }
            }
          },
          "403": {
            "description": "Missing permission"
          },
          "503": {
            "description": "Service Unavailable"
          }
        },
        "security": [
          {
            "ValidateFlowwerAuthenticationScheme": [ ]
          }
        ],
        "x-ms-visibility": "important"
      }
    },
    "/api/v1/documents/{DocumentId}/approve": {
      "post": {
        "tags": [
          "Documents"
        ],
        "summary": "Approves single document",
        "description": "Requires ApproveDocuments permission.",
        "operationId": "Approve",
        "consumes": [
          "application/json-patch+json",
          "application/json",
          "text/json",
          "application/*+json"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "DocumentId",
            "description": "ID of the document to be approved.",
            "required": true,
            "type": "integer",
            "format": "int64"
          },
          {
            "in": "body",
            "name": "body",
            "description": "'AtStage' is required to ensure approval goes really to the desired stage (optimistic concurrency).",
            "schema": {
              "$ref": "#/definitions/ApproveDocumentData"
            }
          }
        ],
        "responses": {
          "401": {
            "description": "Unauthorized"
          },
          "200": {
            "description": "Approval was processed successfully"
          },
          "404": {
            "description": "Returned if user was not found"
          },
          "403": {
            "description": "Forbidden"
          }
        },
        "security": [
          {
            "ValidateFlowwerAuthenticationScheme": [ ]
          }
        ],
        "x-ms-visibility": "important"
      }
    },
    "/api/v1/documents/{DocumentId}": {
      "get": {
        "tags": [
          "Documents"
        ],
        "summary": "Get the DocumentStatus of a single FLOWWER-document",
        "description": "",
        "operationId": "GetDocumentStatus",
        "produces": [
          "text/plain",
          "application/json",
          "text/json"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "DocumentId",
            "description": "ID of the document.",
            "required": true,
            "type": "integer",
            "format": "int64"
          }
        ],
        "responses": {
          "401": {
            "description": "Unauthorized"
          },
          "200": {
            "description": "Status-Data of the document",
            "schema": {
              "$ref": "#/definitions/DocumentStatusInfo"
            }
          },
          "403": {
            "description": "Missing permission"
          }
        },
        "security": [
          {
            "ValidateFlowwerAuthenticationScheme": [ ]
          }
        ],
        "x-ms-visibility": "important"
      }
    },
    "/api/v1/download/{DocumentId}/download/{targetFilename}": {
      "get": {
        "tags": [
          "Download"
        ],
        "summary": "Downloads the document (as uploaded - without FLOWWER-Log) in Browser",
        "description": "When calling from browser, you can leave targetFilename empty to get preview instead of SaveAs-Dialog",
        "operationId": "DownloadDocument",
        "produces": [
          "application/octet-stream"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "DocumentId",
            "description": "ID of the document.",
            "required": true,
            "type": "integer",
            "format": "int64"
          },
          {
            "in": "path",
            "name": "targetFilename",
            "description": "TargetFilename for Browser's previewmode",
            "type": "string"
          },
          {
            "in": "query",
            "name": "uniqueId",
            "description": "Unique key-value required for access permission.",
            "required": true,
            "type": "string",
            "format": "uuid"
          },
          {
            "in": "query",
            "name": "attachmentId",
            "description": "Optional Attachment-ID to address a document's attachment instead of the document itself",
            "type": "integer",
            "format": "int32"
          }
        ],
        "responses": {
          "200": {
            "description": "PDF-Data of the requested document or attachment.",
            "schema": {
              "format": "binary",
              "type": "file"
            }
          },
          "404": {
            "description": "Nothing if document or attachment was not found or GUID does not match"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          }
        },
        "security": [
          {
            "ValidateFlowwerAuthenticationScheme": [ ]
          }
        ]
      }
    },
    "/api/v1/upload": {
      "post": {
        "tags": [
          "Upload"
        ],
        "summary": "Upload a file to FLOWWER",
        "description": "UploadDocuments-permission must be granted.<br />\r\nUse UploadMultipart to provide more information with the uploaded file.<br />\r\nUploaded data must be at least 10 Bytes of size.",
        "operationId": "Upload",
        "consumes": [
          "application/octet-stream"
        ],
        "produces": [
          "text/plain",
          "application/json",
          "text/json"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "FlowwId",
            "description": "If set, the uploaded document will be assigned to the Floww having this Id. Mandatory if site settings require Floww assignment on Upload.",
            "type": "integer",
            "format": "int32"
          },
          {
            "in": "query",
            "name": "CompanyId",
            "description": "If set, the uploaded document will be assigned to the company having this Id.",
            "type": "integer",
            "format": "int32"
          },
          {
            "in": "query",
            "name": "Filename",
            "description": "The filename of the uploaded document will be set to this value. If empty, a filename from the current DateTime will created.",
            "type": "string"
          },
          {
            "in": "body",
            "name": "body",
            "required": true,
            "schema": {
              "format": "binary",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "ElementInfo of the created Document",
            "schema": {
              "$ref": "#/definitions/DocumentUploadResult"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          }
        },
        "security": [
          {
            "ValidateFlowwerAuthenticationScheme": [ ]
          }
        ],
        "x-ms-visibility": "important"
      }
    }
  },
